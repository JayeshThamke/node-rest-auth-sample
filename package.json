{
  "name": "rest-auth-sample",
  "version": "0.0.0",
  "scripts": {
    "clean": "shx rm -rf dist",
    "copy": "shx mkdir dist && shx cp -r public views dist/",
    "compile": "npm run clean && npm run copy && babel \"./{,!(dist|node_modules)/**/}*.js\" bin/www -d dist",
    "start": "npm run compile && node ./dist/bin/www",
    "test": "npm run clean && \"./node_modules/.bin/mocha\" --timeout 5000 --require babel-core/register \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.10",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "shx": "^0.3.2",
    "sinon": "^6.1.5",
    "supertest": "^3.1.0"
  },
  "author": {
    "name": "Brij Mohan",
    "url": "https://techbrij.com"
  }
}
